{
    "root":true,
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": ["error", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }],
        "no-multi-spaces": ["error"],
        "no-undef": "off",
        "no-console": "off",
        "camelcase": ["error", { "properties": "never", "ignoreDestructuring": true }]
    }
}
